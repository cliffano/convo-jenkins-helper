{
  "name": "convo-jenkins-helper",
  "version": "0.0.1",
  "description": "Convo helper for Jenkins",
  "keywords": [
    "convo",
    "jenkins",
    "cloudfunctions",
    "dialogflow"
  ],
  "main": "./lib/index",
  "scripts": {
    "test": "node_modules/mocha/bin/mocha --timeout 5000 test/integration/*/*/*.js",
    "coverage": "node_modules/mocha/bin/mocha --require mocha-lcov-reporter --reporter mocha-lcov-reporter --timeout 5000 test/integration/*/*/*.js",
    "publish-coverage": "cat stage/coverage.lcov | node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "MIT"
}
