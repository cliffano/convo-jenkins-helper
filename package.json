{
  "name": "convo-jenkins-helper",
  "version": "0.0.2",
  "description": "Helper node.js module for Convo Jenkins",
  "keywords": [
    "convo",
    "jenkins"
  ],
  "main": "./lib/index",
  "scripts": {
    "test": "node_modules/.bin/mocha --timeout 5000 test/*/*.js",
    "coverage": "node_modules/.bin/istanbul cover --dir stage/coverage _mocha -- test/*/*.js",
    "publish-coverage": "cat stage/coverage.lcov | node_modules/coveralls/.bin/coveralls"
  },
  "dependencies": {
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "license": "MIT"
}
